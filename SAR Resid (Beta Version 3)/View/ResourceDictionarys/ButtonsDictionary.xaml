                        <Border x:Name="SubBDR" Width="50" Height="50" CornerRadius="50" Background="#6EFFFFFF">
                            <icons:PackIconIonicons Foreground="Black" Kind="PersoniOS" HorizontalAlignment="Center" VerticalAlignment="Center" Height="30" Width="25"/>
                        </Border>
                    </Border>
                    <ControlTemplate.Triggers>
