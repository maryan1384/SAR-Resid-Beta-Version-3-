                <ControlTemplate>
                    <Border x:Name="BDR" Width="60" Height="60" CornerRadius="50" Background="#6EFFFFFF" Margin="0 4 0 0">
                        <Border x:Name="SubBDR" Width="50" Height="50" CornerRadius="50" Background="#6EFFFFFF">
                            <icons:PackIconMaterial Foreground="Black" Kind="Calculator" HorizontalAlignment="Center" VerticalAlignment="Center" Height="40" Width="22"/>
                        </Border>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Background" TargetName="SubBDR" Value="#FFFFFFFF"/>
