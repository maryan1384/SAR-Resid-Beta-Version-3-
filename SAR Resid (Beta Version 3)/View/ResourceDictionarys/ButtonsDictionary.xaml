                                        <DoubleAnimation Storyboard.TargetName="SubBorder" Storyboard.TargetProperty="Height" To="38" Duration="0:0:0:0.01"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="SubBorder" Storyboard.TargetProperty="Width" To="37" Duration="0:0:0:0.01"/>
                                        <DoubleAnimation Storyboard.TargetName="SubBorder" Storyboard.TargetProperty="Height" To="37" Duration="0:0:0:0.01"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Trigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="SubBorder" Storyboard.TargetProperty="Width" To="37" Duration="0:0:0:0.01"/>
                                        <DoubleAnimation Storyboard.TargetName="SubBorder" Storyboard.TargetProperty="Height" To="37" Duration="0:0:0:0.01"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="SubBorder" Storyboard.TargetProperty="Width" To="38" Duration="0:0:0:0.01"/>
                                        <DoubleAnimation Storyboard.TargetName="SubBorder" Storyboard.TargetProperty="Height" To="38" Duration="0:0:0:0.01"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
